            <table style="
            box-sizing: border-box;
            width: 100%;
            background-color: rgb(217, 226, 243);
          ">
                <tr style="font-size: 6.5px">
                    <td style="
                width: 3.6633333821777785%;
                text-align: center;
                vertical-align: middle;
                border-right: 1px solid black;
                font-family: Arial, Helvetica, sans-serif;
              "></td>
                    <td style="
                text-align: center;
                vertical-align: middle;
                border-right: 1px solid black;
                box-sizing: border-box;
                padding: 1.0466666806222225%;
                font-family: Arial, Helvetica, sans-serif;
              "></td>
                    <td style="
                width: 29.934667065795562%;
                text-align: left;
                font-weight: 600;
                font-family: Arial, Helvetica, sans-serif;
                padding-top: 0.418666672248889%;
                padding-left: 0.418666672248889%;
                box-sizing: border-box;
                color: rgb(21, 123, 197);
                font-size: 8px;
              ">
                        <table style="
                  box-sizing: border-box;
                  width: 100%;
                  font-family: Arial, Helvetica, sans-serif;
                ">
                            <tr class="nested_table_row">
                                <td style="
                      width: 17.793333570577783%;
                      font-weight: 600;
                      font-family: Arial, Helvetica, sans-serif;
                      box-sizing: border-box;
                      color: rgb(21, 123, 197);
                      font-size: 8px;
                    ">
                                    FINAL AMT DUE
                                </td>
                                <td style="
                      text-align: right;
                      width: 10.466666806222225%;
                      font-family: Arial, Helvetica, sans-serif;
                      box-sizing: border-box;
                      color: rgb(21, 123, 197);
                      font-size: 8px;
                      padding-right: 0.418666672248889%;
                      padding-right: 1px;
                    ">
                                    <%= addCommas(clientData.final_bal_due)%>
                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
            </table>
        </div>
    </div>
    <div style="float: left; margin-top: 45%; margin-left: 2.366666806222225%">
